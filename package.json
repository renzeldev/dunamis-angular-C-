  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start::local": "ng serve --proxy-config proxy-local.conf.json",
    "start::test": "ng serve --proxy-config proxy-test-azure.conf.json --optimization=false --aot=false",
    "start::feature": "ng serve --proxy-config proxy-feature-conf.json --aot=false",
    "build": "ng build",
    "build::prod": "ng build --prod",
    "build:ssr": "npm run build -- --app=ssr --output-hashing=media",
    "test": "ng test --code-coverage",
    "test::api": "ts-node node_modules/jasmine/bin/jasmine --config=./api/jasmine.json",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "e2e": "ng e2e",
    "create-icon-sprite": "svg-symbols ./src/assets/images/icons > ./src/assets/images/sprites/sprite.svg"
  },
  "private": true,
